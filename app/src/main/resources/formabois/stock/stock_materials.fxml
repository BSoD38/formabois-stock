<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="formabois.stock.controllers.MaterialStockController">
   <HBox alignment="TOP_CENTER" prefWidth="200.0">
      <Label text="Entrées de stock - Matériaux bruts" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </HBox>
   <SplitPane dividerPositions="0.25, 0.7506" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <AnchorPane>
         <VBox alignment="TOP_CENTER" prefHeight="598.0" prefWidth="221.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Label text="Menu">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button layoutX="35.0" layoutY="47.0" mnemonicParsing="false" onAction="#gotoMaterialArrivals" text="Entrées de stock de matériaux" />
            <Button mnemonicParsing="false" onAction="#gotoMaterialDepartures" text="Sorties de stock de matériaux" />
            <Button defaultButton="true" layoutX="71.0" layoutY="37.0" mnemonicParsing="false" text="Liste des stocks de matériaux" />
            <Button layoutX="28.0" layoutY="117.0" mnemonicParsing="false" text="Gestion des matériaux" />
            <Line endX="100.0" startX="-100.0" />
            <Button layoutX="35.0" layoutY="47.0" mnemonicParsing="false" text="Sorties de stock de produits finis" />
            <Button layoutX="27.0" layoutY="117.0" mnemonicParsing="false" text="Entrées de stock de produits finis" />
            <Button layoutX="28.0" layoutY="82.0" mnemonicParsing="false" text="Liste des stocks de produits finis" />
            <Button layoutX="19.0" layoutY="268.0" mnemonicParsing="false" text="Gestions des produits finis" />
            <Line endX="100.0" startX="-100.0" />
            <Button layoutX="64.0" layoutY="82.0" mnemonicParsing="false" text="Statistiques" />
         </VBox>
      </AnchorPane>
      <AnchorPane id="Content" minHeight="-1.0" minWidth="-1.0" prefHeight="570.0" prefWidth="443.0">
         <TableView fx:id="materialArrivalsTable" prefHeight="569.0" prefWidth="501.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
               <TableColumn text="Nombre">
                  <cellValueFactory>
                     <PropertyValueFactory property="count" />
                  </cellValueFactory>
               </TableColumn>
               <TableColumn prefWidth="150.0" text="Site">
                  <cellValueFactory>
                     <PropertyValueFactory property="site" />
                  </cellValueFactory>
               </TableColumn>
               <TableColumn prefWidth="186.0" text="Matériau">
                  <cellValueFactory>
                     <PropertyValueFactory property="material" />
                  </cellValueFactory>
               </TableColumn>
            </columns>
         </TableView>
      </AnchorPane>
      <AnchorPane>
         <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <HBox alignment="CENTER" prefWidth="200.0">
               <Label text="Filtrage" textAlignment="CENTER">
                  <font>
                     <Font size="18.0" />
                  </font>
               </Label>
            </HBox>
            <ScrollPane layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="222.0">
               <VBox alignment="TOP_CENTER" prefHeight="256.0" prefWidth="219.0" spacing="10.0">
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                     <HBox>
                        <Label prefHeight="27.0" prefWidth="78.0" text="Matériau">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Label>
                        <ChoiceBox fx:id="materialFilter" prefWidth="150.0" />
                     </HBox>
                     <HBox>
                        <Label prefHeight="27.0" prefWidth="78.0" text="Site">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Label>
                        <ChoiceBox fx:id="siteFilter" prefWidth="150.0" />
                     </HBox>
                  </VBox>
                  <HBox alignment="CENTER" prefHeight="47.0" prefWidth="219.0">
                     <Button mnemonicParsing="false" onAction="#updateTable" text="Filtrer" />
                  </HBox>
               </VBox>
            </ScrollPane>
         </VBox>
      </AnchorPane>
   </SplitPane>
</VBox>
